" style="margin-bottom:'+t.vindent+'"></div>'),o}function t_feed_showMore(e,t){var o=$("#rec"+e),a=o.find(".js-feed-container"),s=1,d=o.find(".js-feed-btn-show-more");null!==d&&(d.on("click",function(o){o.preventDefault(),window.tFeedPosts[e].switcher?s=2:s++,window.tFeedPosts[e].switcher=!1,t_feed_loadPosts(e,t,s,!0)}),t.isHorizOnMob&&a.bind("scroll",t_throttle(function(){var e=a.get(0).scrollWidth,t=a.scrollLeft(),o=a.outerWidth();o+t+20>e&&"none"!==d.css("display")&&d.click()},200)))}function t_feed_initFeedParts(e,t){var o=$("#rec"+e);o.find(".js-feed-parts-switcher").on("click",function(){var a=$(this).attr("data-feedpart-uid");o.find(".t-active").removeClass("t-active"),$(this).addClass("t-active"),t.feeduid=t.feeduid.split("-")[0]+"-"+a,window.tFeedPosts[e].switcher=!0,window.tFeedPosts[e].count=0,o.find(".t-feed__parts-switch-btn-all").hasClass("t-feed__parts-switch-btn_hide")&&!t.showPartAll&&o.find(".t-feed__parts-switch-btn-all").removeClass("t-feed__parts-switch-btn_hide"),t_feed_showLoadersForPostsList(e),t_feed_loadPosts(e,t,1,!0),$(this).hasClass("t-feed__parts-switch-btn-all")&&history.replaceState({},null,window.location.href.split("#")[0])})}function t_feed_showLoadersForPostsList(e){var t=$("#rec"+e);t.find(".t-feed__post").addClass("t-feed__post_hidden"),t.find(".t-feed__show-more-btn").addClass("t-feed__show-more-btn_hidden");var o=setTimeout(function(){t.find(".js-feed-preloader").css({display:"",opacity:"0"}).animate({opacity:1},200)},1e3);t.data("preloader-timeout",o)}function t_feed_drawWholePost(e,t,o,a){var s=$("#rec"+e),d=s.find(".js-feed").attr("data-feed-grid-type"),r=s.find(".js-feed").attr("data-feed-col-modification"),i=t.directlink?t.directlink:"#postpopup",p="_blank"==t.directtarget?'target="_blank"':"",_="left"===o.align?"t-align_left":"t-align_center",n=o.itemsAnim&&o.previewmode&&!a?"t-animate":"";$(".t-records").attr("data-blocks-animationoff")&&(n="");var l=o.itemsAnim?'data-animate-style="'+o.itemsAnim+'" data-animate-chain="